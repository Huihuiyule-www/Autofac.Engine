<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <SignAssembly>false</SignAssembly>
    <ApplicationIcon />
    <Win32Resource />
    <AssemblyVersion>4.6.0.0</AssemblyVersion>
    <FileVersion>4.6.0.0</FileVersion>
    <PackageReleaseNotes>Autofac engine, MS Surfing, MSSurfing.Api</PackageReleaseNotes>
    <Company>MSSurfing.Api</Company>
    <Authors>MSSurfing.Api</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/MSSurfing/Autofac.Engine</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MSSurfing/Autofac.Engine</RepositoryUrl>
    <PackageIconUrl>https://github.com/MSSurfing/Autofac.Engine</PackageIconUrl>
    <Version>4.6.0</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>Samples ：https://github.com/MSSurfing/Autofac.Engine/tree/master/Samples
Samples Code : {
   Init : Startup.ConfigureServices(){ 
         return EngineContext.Initialize(services, ScopeTag.Http); 
   },
   Registrar : A:IDependencyRegistrar { builder.RegisterType&lt;Service&gt;().InstancePerLifetimeScope(); }
   Resolve(Anywhere) : EngineContext.Resolve&lt;IService&gt;();
   Resolve(New Lifetime) : EngineContext.BeginLifetimeScope().Resolve&lt;IService&gt;();
}</Description>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageTags>Autofac engine, MS Surfing, MSSurfing.Api</PackageTags>
    <PackageLicenseUrl>None</PackageLicenseUrl>
    <Copyright>None</Copyright>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|netstandard2.0|AnyCPU'">
    <DefineConstants>TRACE;DEBUG</DefineConstants>
    <DocumentationFile>bin\Debug\netstandard2.0\Autofac.Engine.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|netstandard2.0|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard2.0\Autofac.Engine.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="4.2.1" />
    <PackageReference Include="Autofac.Extensions.DependencyInjection" Version="4.2.2" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="Microsoft.Extensions.DependencyInjection">
      <Version>2.0.0</Version>
    </PackageReference>
  </ItemGroup>

</Project>
